    \@tginputcentrefalse\@tginputlignefalse\@tginputnumerofalse%
    \@tfor \@tempa :=#1\do%
        {\if\@tempa c\@tginputcentretrue\fi%
        \if\@tempa l\@tginputlignetrue\fi%
