        \par\fi
        \if@tginputcentre\vbox\bgroup\fi% Le vbox implique le vspace ci-dessus.
        \vbox{\noindent\rule{\hsize}{\arrayrulewidth}}\nopagebreak%
        \if@tginputcentre\vspace{-\abovedisplayskip}\nopagebreak\fi%
    \else
        \ifhmode
